<%= render "components/header/header" %>
<div class="page container">
  <div class="row">
    <div class="col-md-8 offset-2 page--title">
      <h1><%= title %></h1>
    </div>
  </div>
  <%= yield %>
  <% if defined?(objects) %>
    <div class="page--pagination">
      <%= paginate objects %>
    </div>
  <% end %>
</div>
<%= render "components/footer/footer" %>
